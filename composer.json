{
    "name": "zfcampus/zf-rest",
    "description": "ZF2 Module providing structure for RESTful resources",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "rest"
    ],
    "homepage": "https://bitbucket.org/zendframework/zfrest",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:zendframework/zfapiproblem.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:zendframework/zfhal.git"
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zfcampus/zf-api-problem": "dev-master",
        "zfcampus/zf-hal": "dev-master",
        "zendframework/zend-eventmanager": ">=2.3.0dev",
        "zendframework/zend-json": ">=2.3.0dev",
        "zendframework/zend-loader": ">=2.3.0dev",
        "zendframework/zend-mvc": ">=2.3.0dev",
        "zendframework/zend-paginator": ">=2.3.0dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "zendframework/zend-console": ">=2.3.0dev",
        "zendframework/zend-http": ">=2.3.0dev",
        "zendframework/zend-servicemanager": ">=2.3.0dev",
        "zendframework/zend-uri": ">=2.3.0dev",
        "zendframework/zend-view": ">=2.3.0dev"
    },
    "autoload": {
        "psr-0": {
            "ZF\\Rest\\": "src/",
            "ZFTest\\": "test/"
        }
    }
}
